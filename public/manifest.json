{
  "name": "Koinly Extensions",
  "description": "Adds CSV Export for https://koinly.io",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
     "activeTab",
     "scripting",
     "storage"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Koinly Extensions",
    "default_icon": {
       "16": "/images/logo_16.png",
       "32": "/images/logo_32.png",
       "48": "/images/logo_48.png",
       "128": "/images/logo_128.png"
     }
  },
  "icons": {
    "16": "/images/logo_16.png",
    "32": "/images/logo_32.png",
    "48": "/images/logo_48.png",
    "128": "/images/logo_128.png"
  },
  "host_permissions": [
    "https://koinly.io/"
  ]
}